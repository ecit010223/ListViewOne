<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/optional_scroll_tab"
    android:background="@color/demand_efefef"
    android:orientation="horizontal" >

    <TextView
        android:id="@+id/item1"
        android:layout_width="@dimen/list_item_width_mid_100dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:minLines="2"
        android:text="@string/IDS_QiQuanName"
        android:textColor="@color/gray_demand_one"
        android:textSize="@dimen/font_t_offer_D" >
    </TextView>

    <LinearLayout
        android:id="@+id/head"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >
    </LinearLayout>

</LinearLayout>